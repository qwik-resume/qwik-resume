{
  "name": "qwik-resume",
  "version": "0.0.0",
  "description": "A dynamic and innovative application built using the Qwik framework and artificial intelligence to generate customizable, job-specific résumés.",
  "engines": {
    "node": ">=18.0.0"
  },
  "private": true,
  "scripts": {
    "build": "qwik build",
    "build-storybook": "storybook build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.types": "tsc --incremental --noEmit",
    "deploy": "echo 'Run \"npm run qwik add\" to install a server adapter'",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\" --fix",
    "preview": "qwik build preview && vite preview --open",
    "start": "vite --open --mode ssr",
    "storybook": "storybook dev -p 6006",
    "test.unit": "vitest components",
    "test.unit.ui": "vitest --ui components",
    "qwik": "qwik"
  },
  "keywords": [
    "resume",
    "cv",
    "qwik",
    "qwik resume",
    "employment",
    "job search",
    "compose resume"
  ],
  "contributors": [
    {
      "name": "Chris Strolia-Davis",
      "email": "full-stack.dev@strolia-davis.us"
    }
  ],
  "license": "MIT",
  "dependencies": {
    "@builder.io/qwik": "^1.2.6",
    "@builder.io/qwik-city": "^1.2.6",
    "@storybook/addon-essentials": "7.0.23",
    "@storybook/addon-links": "7.0.23",
    "@storybook/addon-styling": "^1.3.2",
    "@storybook/blocks": "7.0.23",
    "@storybook/builder-vite": "7.0.23",
    "@storybook/html": "7.0.23",
    "@storybook/html-vite": "7.0.23",
    "@types/eslint": "8.40.2",
    "@types/node": "^20.4.0",
    "@typescript-eslint/eslint-plugin": "5.61.0",
    "@typescript-eslint/parser": "5.61.0",
    "@vitest/ui": "^0.32.2",
    "autoprefixer": "^10.4.14",
    "eslint": "8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-fp": "^2.3.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-qwik": "^1.2.6",
    "postcss": "^8.4.23",
    "prettier": "2.8.8",
    "storybook": "7.0.23",
    "storybook-framework-qwik": "0.2.2",
    "tailwindcss": "^3.3.1",
    "typescript": "5.1.6",
    "undici": "5.22.1",
    "vite": "4.4.1",
    "vite-tsconfig-paths": "4.2.0",
    "vitest": "^0.32.2"
  }
}
